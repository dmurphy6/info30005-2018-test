<div class="form">
    <!-- show any messages that come back-->
    <% if (message.length > 0) { %>
    <div class="alert alert-danger"><%= message %></div>
    <% } %>

    <form  method="post" action="secondInfo">
        <div class="row justify-content-center">
            <h1>About You</h1>
        </div>

        <div class="row">
            <div class="col-sm-4 labelInformation">
                <label for="aboutYourself">About Yourself</label>
            </div>
        </div>
        <div class="col-sm-10">
            <textarea name="aboutYourself" placeholder="Say something" required><%=about%></textarea>
        </div>

        <div class="row">
            <div class="col-sm-4 labelInformation">
                <label for="aboutYourFamily">About Your Family</label>
            </div>
        </div>
        <div class="col-sm-10">
            <textarea name="aboutYourFamily" placeholder="Describe your family" required><%=aboutFamily%></textarea>
        </div>

        <div class="row">
            <div class="col-sm-4 labelInformation">
                <label for="highestEducation">Highest Education</label>
            </div>
        </div>
        <div class="col-sm-10">
            <input type="text" name="highestEducation" value=<%=highestEducation%> >
        </div>

        <div class="row">
            <div class="col-sm-5 labelInformation">
                <label for="achievement">Achievement in Career</label>
            </div>
        </div>
        <div class="col-sm-10">
            <textarea name="achievement"><%=achievementInCareer%></textarea>
        </div>

        <div class="row">
            <div class="col-sm-5 labelInformation">
                <label for="motto">Motto</label>
            </div>
        </div>
        <div class="col-sm-10">
            <textarea name="motto"><%=motto%></textarea>
        </div>

        <div class="registerButton">
            <input type="submit" value="Next">
        </div>
    </form>
</div>


